<div id="access-control-sticker" class="box you access-control hidden" style="position: absolute; width: 620px; z-index: 300;">
  <div class="stickerMetadata" style="display: none;">
    <input type="hidden" id="stickerResourceURI" name="stickerResourceURI" value="" />
    <input type="hidden" id="stickerIconURI" name="stickerIconURI" value=" />
    <span class="merged-stickers-metadata">
    </span>
  </div>
  <div class="icon" style="width: 60px; height: 60px;"><a id="selectStickerIcon" class="sticker_link"><img src="stickers_icons/sticker_new.png" title="Sticker Icon" class="sticker_icon" style="display: block; margin-left: auto; margin-right: auto;"></a></div>
  <ul class="sticker-tabs" style="float: left; left: 68px; width: 85%;">
    <li style="float: left;">
      <h3 id="sticker-title" class="editableTitle click" style="top: 0px; left: 0px;"></h3>
    </li>
  </ul>
  <div class="sticker-wrap">
    <div class="sticker-controls">
        <input type="image" id="close_ac" title="Cancel" src="sticker_container_files/cross.gif" style="float: right; vertical-align: top; margin-left: 0.5em;"/>
        <input type="image" id="save_ac" title="Save" src="sticker_container_files/disk.gif" style="float: right; vertical-align: top; margin-left: 0.5em;"/>
    </div>

    <div class="panel">
    <h3 style="left: 0px; font-size:13px;"><img title="Collection" src="sticker_container_files/photos.png"> Collection <a id="collection_link"><img title="Add Another Collection Item" src="sticker_container_files/plus.png"></a></h3>
    <div class="full collection collection_container" style="width: 505px; margin-top:15px;">
    <table cellspacing="0" cellpadding="0" border="0" style="width: auto;">
    <tbody>
        <tr style="vertical-align: top;">
            <td class="main_content">
            <div id="main_album_content" style="overflow-y: auto; overflow-x: hidden; height: 212px;">
            <div class="content_wrapper collection_container">
            <ul id="collection" style="overflow: hidden; width: 100%; margin: 0px; padding: 0px;">
            </ul>
            </div>
            </div>
            </td>
        </tr>
    </tbody>
    </table> 
    </div>
    <br/>
    </div><!-- Closer of "panel" -->
    
  </div><!-- Close of "sticker-wrap" -->
</div>